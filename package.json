{
  "name": "clawsofsaurtopia",
  "version": "3.0.0",
  "description": "Claws Of Saurtopia Noise festival 2024 // June 7 and June 8 in Leipzig.",
  "main": "index.html",
  "scripts": {
    "watch:sass": "npx sass assets/scss:_site/assets/css --watch",
    "build:sass": "npx sass assets/scss:_site/assets/css",
    "watch:tailwind": "tailwindcss -i assets/css/tailwind.css -o ./_site/assets/css/tailwind.css --watch",
    "build:tailwind": "tailwindcss -i assets/css/tailwind.css -o ./_site/assets/css/tailwind.css --minify",
    "build": "npx @11ty/eleventy & npm run build:sass & npm run build:tailwind",
    "dev": "npx @11ty/eleventy --watch & npm run watch:sass & npm run watch:tailwind"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/and-kal/ClawsOfSaurtopia.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/and-kal/ClawsOfSaurtopia/issues"
  },
  "homepage": "https://clawsofsaurtopia.glitch.me",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "hydra-synth": "^1.3.29",
    "sass": "^1.62.1"
  }
}
